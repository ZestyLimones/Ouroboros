<h2>Welcome, {{first_name}}</h2>
<h3>{{setting.bio}}</h3>
<a href="/daily_log">
<button>Daily Log</button>
</a>
<!--Temp Div for variables-->
<div>
  {{#each user_activities as |activity|}}
  <p>{{activity.name}}: - {{activity.userPercent}} %</p>
  {{/each}} 
</div>


<div class="progress" data-percent="80">
  <div class="skill">
  <div class="outer">
    <div class="inner">
      <div id="number">
      </div>
    </div>
  </div>
  <!-- this will be customized later! -->
  <svg xmlns="http://www.w3.org/2000/svg" version="1.1" width="160px" height="160px">
         <defs>
            <linearGradient id="GradientColor">
               <stop offset="0%" stop-color="#c3446f" />
               <stop offset="50%" stop-color="#95965e" />
               <stop offset="100%" stop-color="#5e967b" />
            </linearGradient>
         </defs>
         <circle class="anim-80" cx="80" cy="80" r="70" stroke-linecap="round" />
 </svg> 
 </div>

</div>


<script src = "/js/progress-bar.js"></script>
<a href="/account_details">
<button>Account Settings</button>
</a>